<h3><%=params[:nickname] %></h3>

<table border=1>
  <tr>
    <th></th>
    <th>主催者</th>
    <th>イベント</th>
  </tr>
  <% @events.each_with_index{ |event,index| %>
      <tr>
        <td><span class="badge"><%= index+1 %></span></td>
        <td><%= event['owner_nickname'] %></td>
        <td><%= link_to_different_domain( event['title'], event['event_url'] ) %></td>
      </tr>
  <% } %>
</table>
